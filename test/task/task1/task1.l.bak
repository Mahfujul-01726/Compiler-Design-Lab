%{
#include <stdio.h>
int word_count = 0;
%}

%%
[ \t\n]+   ;              // Ignore spaces, tabs, newlines
[A-Za-z0-9]+  { word_count++; }   // Count words (alphanumeric sequences)
.   ;                      // Ignore other characters
%%

int main() {
    yylex();
    printf("Number of words: %d\n", word_count);
    return 0;
}

int yywrap() {
    return 1;
}
